{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./build",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true
  },
"include": ["src/**/*", "prisma/**/*"],
  "exclude": ["node_modules", "prisma/migrations"],
  "ts-node": {
    "swc": true
  }
}